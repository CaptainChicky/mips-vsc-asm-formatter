{
    "name": "asmformat",
    "publisher": "CC",
    "displayName": "MIPS Assembly Formatter",
    "description": "Formats MIPS Assembly Code",
    "version": "1.2.0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onLanguage:mips",
        "onLanguage:asm"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "mips",
                "extensions": [".asm", ".s"],
                "aliases": ["MIPS", "MIPS Assembly", "Assembly", "asm"]
            }
        ]
    },
    "bugs": {
        "email": "scarletseallawyer@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/CaptainChicky/mips-vsc-asm-formatter.git"
    },
    "homepage": "https://github.com/CaptainChicky/mips-vsc-asm-formatter"
}