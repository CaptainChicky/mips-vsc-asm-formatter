{
	"name": "asmformat",
	"publisher": "CC",
	"displayName": "MIPS Assembly Formatter",
	"description": "Formats MIPS Assembly Code",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:asmformat.format"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "asmformat.format",
				"title": "Format Asm"
			}
		],
		"menus": {
			"editor/context": [
			  {
				"when": "resourceExtname == .asm",
				"command": "asmformat.format",
				"group": "navigation"
			  }
			]
		  }
	},
	"scripts": {
		"lint": "eslint ."
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3"
	},
	"bugs": {
		"email": "scarletseallawyer@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/CaptainChicky/mips-vsc-asm-formatter.git"
	},
	"homepage": "https://github.com/CaptainChicky/mips-vsc-asm-formatter"
}